{
  "name": "compoza",
  "version": "1.0.0",
  "private": true,
  "description": "Docker management for the discerning individual",
  "license": "MIT",
  "author": "Lyall Cooper",
  "homepage": "https://github.com/lyallcooper/compoza",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyallcooper/compoza"
  },
  "bugs": {
    "url": "https://github.com/lyallcooper/compoza/issues"
  },
  "keywords": [
    "docker",
    "compose",
    "management",
    "web-ui",
    "self-hosted"
  ],
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:next": "next dev",
    "build": "next build && tsc -p server/tsconfig.json",
    "start": "node server/dist/index.js",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "demo": "NEXT_PUBLIC_DEMO=true next dev",
    "demo:no-banner": "NEXT_PUBLIC_DEMO=true NEXT_PUBLIC_DEMO_BANNER=false next dev",
    "build:demo": "NEXT_PUBLIC_DEMO=true next build"
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.12.1",
    "@codemirror/state": "^6.5.4",
    "@codemirror/view": "^6.39.12",
    "@lezer/highlight": "^1.2.3",
    "@tanstack/react-query": "^5.90.20",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "codemirror": "^6.0.2",
    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "next": "16.1.6",
    "picocolors": "^1.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.16.3",
    "@tailwindcss/postcss": "^4",
    "@types/dockerode": "^4.0.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "cpu-features",
      "esbuild",
      "ssh2"
    ],
    "ignoredBuiltDependencies": [
      "sharp",
      "unrs-resolver"
    ]
  }
}
